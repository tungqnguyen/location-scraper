{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-unused-vars": "off",
        "array-callback-return":"off",
        "no-console":"off",
        "import/no-extraneous-dependencies":"off",
        "consistent-return":"off",
        "eqeqeq":"off",
        "max-len":["error", {"code": 150, "ignoreTemplateLiterals": true, "ignoreStrings": true}]
    }
}